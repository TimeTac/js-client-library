# TimeTac EditorConfig for JS Projects
# reference: https://editorconfig.org/

# top-most EditorConfig file in this repository
root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
insert_final_newline = true
indent_size = 2

[*.{md,markd,markdown}]
max_line_length = 80
trim_trailing_whitespace = false
